<table id="car_table">
      <tr>
        <th onclick="sort_car_table(0)">Brand</th>
        <th onclick="sort_car_table(1)">Model</th>
      </tr>
  <% @cars.each do |brand, cars| %>
    <tr>
      <td>
        <%= brand %>
      </td>
      <% models = Array.new %>
    <td>
      <% cars.each do |key| %>
      <div class="car_model">
        <div class="model_title">
          <%= key["model"]%>
        </div>
        <div class="td-caret tools_menu">
          <div class="dropdown pull-right">
            <button type="button" class="btn btn-default btn-xs dropdown-toggle btn-xs-caret" data-toggle="dropdown"> <span class="caret"></span></button>
            <ul class="dropdown-menu">
                <li><%= link_to "Add car model",  new_admin_car_model_path %></li>
                <li><a href="/admin/car_models/<%= key["id"] %>/edit">Update car model</a></li>
                <li><a href="/admin/car_models/<%= key["id"] %>" data-confirm="Are you sure?" data-method="delete" >Delete car model</a></li>
              <hr>
                <li><%= link_to "Add car brand",  new_admin_brand_path %></li>
                <li><a href="/admin/brands/<%= key["brand_id"] %>/edit">Update car brand</a></li>
                <li><a href="/admin/brands/<%= key["brand_id"] %>" data-confirm="Are you sure?" data-method="delete" >Delete car brand</a></li>
            </ul>
          </div>
        </div>
      </div>
      <% end %>
    </td>
  <% end %>
  </tr>
</table>
